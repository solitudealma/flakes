group {
    main_group {
        filter: subtag(wget) && !name(keyword: 'ExpireAt:')
        policy: min_moving_avg
    }

    us {
        filter: subtag(wget) && name(keyword: '美国' )
        policy: min_moving_avg
    }

    steam {
        filter: subtag(wget) && !name(keyword: 'ExpireAt:')
        policy: min_moving_avg
        tcp_check_url: 'http://test.steampowered.com'
    }
}
